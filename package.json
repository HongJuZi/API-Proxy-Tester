{
  "name": "api-proxy-tester",
  "version": "1.0.0",
  "description": "API代理测试工具",
  "repository": {
    "type": "git",
    "url": "https://github.com/HongJuZi/API-Proxy-Tester.git"
  },
  "main": "src/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:root": "VITE_BASE_DIR=/ VITE_OUT_DIR=dist-root/ vite build",
    "build:custom": "vite build --mode custom",
    "build:specific": "VITE_BASE_DIR=/custom/ VITE_OUT_DIR=dist-custom/ VITE_ASSETS_DIR=static vite build",
    "build:prod": "vite build --mode prod",
    "build:work": "vite build --mode work",
    "serve": "vite preview",
    "serve:root": "VITE_BASE_DIR=/ vite preview"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "axios": "^1.6.0",
    "json5": "^2.2.3",
    "pinia": "^2.1.7",
    "sharp": "^0.34.5",
    "vue": "^3.4.0",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.0"
  },
  "keywords": [
    "api",
    "testing",
    "vue3",
    "tool"
  ],
  "author": "红橘子科技",
  "license": "红橘子科技"
}
